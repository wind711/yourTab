{
    "name": "Your Tab",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Keep track of tabs you want to revisit",
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png"
        }
    },
    "icons": {
        "16": "images/get_started16.png"
    }
}